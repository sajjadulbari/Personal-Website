      "source": [
        "####============================================================================\n",
        "\n",
        "                                # CPU Version\n",
        "                                # Whole code in one cell\n",
        "                                # BM3D and DRUNet added\n",
        "                                #Anscombe Transfrom added\n",
        "\n",
        "\n",
        "####============================================================================\n",
        "\n",
        "\n",
        "from google.colab import drive\n",
        "drive.mount('/content/drive', force_remount=True)\n",
        "\n",
        "import os\n",
        "os.chdir('/content/drive/MyDrive/GovernmentDataset1')\n",
        "\n",
        "# Install required dependencies quietly\n",
        "!pip install PyWavelets --quiet\n",
        "!pip install pytorch-msssim --quiet\n",
        "!pip install bm3d --quiet\n",
        "!pip install torch --quiet\n",
        "\n",
        "import imageio.v2 as imageio\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import pywt\n",
        "import bm3d\n",
        "import torch\n",
        "from pytorch_msssim import ms_ssim\n",
        "from tqdm import tqdm\n",
        "import time\n",
        "from PIL import Image\n",
        "import multiprocessing as mp\n",
        "from functools import partial\n",
        "\n",
        "# DRUNet-specific imports\n",
        "from collections import OrderedDict\n",
        "\n",
        "# Start timer to measure total execution time\n",
        "start_time = time.time()\n",
        "\n",
        "# =============================================================================\n",
        "# Configuration Parameters\n",
        "# =============================================================================\n",
        "config = {\n",
        "    'BASE_DIR': '/content/drive/MyDrive/GovernmentDataset1',\n",
        "    'DRUNET_DIR': '/content/drive/MyDrive/DRUNet Contents',\n",
        "    'DATASET_TYPE': 'HighSNR',     # Options: 'LowSNR', 'MedSNR', 'HighSNR', 'LowBlur', 'MedBlur', 'HighBlur'\n",
        "    'INCLUDE_FULL_RESPONSE': False,\n",
        "    'NUM_IMAGES': 10,\n",
        "    'DENOISER_TYPE': 'DRUNET',  # Options: 'BM3D', 'DRUNET'\n",
        "    'USE_ANSCOMBE': True,      # New flag to enable/disable Anscombe transformation\n",
        "    'MS_SSIM_DATA_RANGE': 1.0,\n",
        "    'DEGREE_LISTS': {\n",
        "        'LowSNR': [38, 68, 82, 83, 90, 91, 122, 132, 145, 180, 203, 207, 227, 260, 262, 271, 293, 312, 353, 355],\n",
        "        'HighSNR': [85, 88, 133, 135, 146, 159, 160, 168, 176, 187, 222, 246, 258, 270, 296, 346, 348, 352, 357, 359],\n",
        "        'MedSNR': [33, 40, 64, 89, 103, 111, 117, 126, 138, 169, 178, 182, 190, 205, 216, 217, 242, 287, 297, 324],\n",
        "        'HighBlur': [0, 6, 18, 27, 52, 83, 123, 125, 153, 187, 206, 207, 214, 273, 274, 290, 313, 315, 342, 356],\n",
        "        'MedBlur': [14, 16, 27, 48, 80, 86, 103, 105, 114, 145, 146, 166, 171, 184, 196, 198, 242, 248, 250, 254],\n",
        "        'LowBlur': [18, 33, 39, 86, 97, 117, 119, 124, 156, 157, 167, 171, 180, 226, 228, 249, 259, 263, 352, 355]\n",
        "    },\n",
        "    'GROUND_TRUTH_MAP': {\n",
        "        'LowSNR': 'SNR_LOW',\n",
        "        'MedSNR': 'SNR_MED',\n",
        "        'HighSNR': 'SNR_HIGH',\n",
        "        'LowBlur': 'BLUR_LOW',\n",
        "        'MedBlur': 'BLUR_MED',\n",
        "        'HighBlur': 'BLUR_HIGH'\n",
        "    },\n",
        "    'DISTANCE_MAP': {\n",
        "        'LowSNR': 1000,\n",
        "        'MedSNR': 2500,\n",
        "        'HighSNR': 4000,\n",
        "        'LowBlur': 200,\n",
        "        'MedBlur': 500,\n",
        "        'HighBlur': 1000\n",
        "    },\n",
        "    'SPECIFIC_DEGREE': None   # Set to None to process all degrees\n",
        "}\n",
        "\n",
        "# Validate DATASET_TYPE\n",
        "if config['DATASET_TYPE'] not in config['DEGREE_LISTS'] or config['DATASET_TYPE'] not in config['GROUND_TRUTH_MAP']:\n",
        "    raise ValueError(f\"Invalid DATASET_TYPE {config['DATASET_TYPE']}. Valid types are: {list(config['DEGREE_LISTS'].keys())}\")\n",
        "\n",
        "# Get the list of degrees for the specified DATASET_TYPE\n",
        "degree_list = config['DEGREE_LISTS'][config['DATASET_TYPE']]\n",
        "if config['SPECIFIC_DEGREE'] is not None:\n",
        "    if config['SPECIFIC_DEGREE'] not in degree_list:\n",
        "        raise ValueError(f\"Invalid SPECIFIC_DEGREE {config['SPECIFIC_DEGREE']}. Valid degrees for {config['DATASET_TYPE']} are: {degree_list}\")\n",
        "    degree_list = [config['SPECIFIC_DEGREE']]\n",
        "print(f\"Processing {config['DATASET_TYPE']} for degrees: {degree_list}\")\n",
        "\n",
        "# Initialize results list for summary table\n",
        "results = []\n",
        "\n",
        "# =============================================================================\n",
        "# Utility Functions\n",
        "# =============================================================================\n",
        "def mse(original, denoised):\n",
        "    return np.mean((original - denoised) ** 2)\n",
        "\n",
        "def psnr(original, denoised, max_pixel=1.0):\n",
        "    m = mse(original, denoised)\n",
        "    if m == 0:\n",
        "        return float('inf')\n",
        "    return 10 * np.log10((max_pixel**2) / m)\n",
        "\n",
        "def anscombe_transform(image):\n",
        "    \"\"\"Apply Anscombe transform: x -> 2 * sqrt(x + 3/8)\"\"\"\n",
        "    return 2 * np.sqrt(np.maximum(image + 3/8, 0))\n",
        "\n",
        "def inverse_anscombe_transform(image):\n",
        "    \"\"\"Apply inverse Anscombe transform: x -> (x/2)^2 - 3/8\"\"\"\n",
        "    return (image / 2) ** 2 - 3/8\n",
        "\n",
        "def detect_inner_box(img):\n",
        "    if img.ndim == 3:\n",
        "        img_gray = (0.299*img[...,0] + 0.587*img[...,1] + 0.114*img[...,2]).astype(np.float64)\n",
        "    else:\n",
        "        img_gray = img.astype(np.float64)\n",
        "\n",
        "    row_mean = img_gray.mean(axis=1)\n",
        "    col_mean = img_gray.mean(axis=0)\n",
        "\n",
        "    t_row = row_mean.max() * 0.95\n",
        "    t_col = col_mean.max() * 0.95\n",
        "\n",
        "    rows_content = row_mean < t_row\n",
        "    cols_content = col_mean < t_col\n",
        "\n",
        "    y_min = int(np.argmax(rows_content))\n",
        "    y_max = int(len(rows_content) - 1 - np.argmax(rows_content[::-1]))\n",
        "    x_min = int(np.argmax(cols_content))\n",
        "    x_max = int(len(cols_content) - 1 - np.argmax(cols_content[::-1]))\n",
        "\n",
        "    inner_corners = [(x_min, y_min), (x_max, y_min), (x_max, y_max), (x_min, y_max)]\n",
        "\n",
        "    return inner_corners, (x_min, y_min, x_max, y_max), row_mean, col_mean\n",
        "\n",
        "def denoise_bm3d(image, sigma):\n",
        "    denoised = bm3d.bm3d(image, sigma, stage_arg=bm3d.BM3DStages.ALL_STAGES)\n",
        "    return denoised\n",
        "\n",
        "def estimate_sigma_masked(image, mask, wavelet='db3', level=3):\n",
        "    coeffs = pywt.swt2(image, wavelet, level=level, start_level=0)\n",
        "    cD_finest = coeffs[-1][1][2]\n",
        "    return np.median(np.abs(cD_finest[mask])) / 0.6745\n",
        "\n",
        "def single2tensor4(img):\n",
        "    img = np.expand_dims(img, axis=2) if img.ndim == 2 else img\n",
        "    img = torch.from_numpy(img).permute(2, 0, 1).unsqueeze(0).float()\n",
        "    return img\n",
        "\n",
        "def tensor2single(img):\n",
        "    img = img.squeeze(0).permute(1, 2, 0).numpy()\n",
        "    if img.shape[2] == 1:\n",
        "        img = img.squeeze(2)\n",
        "    return img\n",
        "\n",
        "def denoise_drunet(image, mask, model, device, sigma_type='wavelet'):\n",
        "    # Normalize image to [0,1]\n",
        "    min_val, max_val = image.min(), image.max()\n",
        "    norm_image = (image - min_val) / (max_val - min_val + 1e-10) if max_val > min_val else image - min_val\n",
        "\n",
        "    # Estimate noise sigma in normalized [0,1] scale\n",
        "    if sigma_type == 'wavelet':\n",
        "        sigma = estimate_sigma_masked(norm_image, mask)\n",
        "    elif sigma_type == 'empirical':\n",
        "        noise_pixels = norm_image[mask]\n",
        "        sigma = np.sqrt(noise_pixels.var())\n",
        "    else:\n",
        "        raise ValueError(f\"Unsupported sigma_type: {sigma_type}\")\n",
        "    sigma_16bit = sigma * 65535.0\n",
        "\n",
        "    # Prepare input for DRUNet\n",
        "    img_L = norm_image\n",
        "    if img_L.ndim == 2:\n",
        "        img_L = np.expand_dims(img_L, axis=2)\n",
        "\n",
        "    img_L_tensor = single2tensor4(img_L)\n",
        "    sigma_tensor = torch.FloatTensor([sigma_16bit / 65535.0]).repeat(1, 1, img_L_tensor.shape[2], img_L_tensor.shape[3])\n",
        "    img_L_tensor = torch.cat((img_L_tensor, sigma_tensor), dim=1).to(device)\n",
        "\n",
        "    # Denoise\n",
        "    with torch.no_grad():\n",
        "        img_E = model(img_L_tensor)\n",
        "\n",
        "    # Convert output to numpy\n",
        "    img_E_float = tensor2single(img_E)\n",
        "    img_E_min = img_E_float.min()\n",
        "    img_E_max = img_E_float.max()\n",
        "    img_E_normalized = (img_E_float - img_E_min) / (img_E_max - img_E_min + 1e-10) if img_E_max > img_E_min else img_E_float - img_E_min\n",
        "\n",
        "    return img_E_normalized, sigma, sigma_16bit\n",
        "\n",
        "def denoise_image(image, mask, denoiser_type, sigma_type='wavelet', model=None, device=None):\n",
        "    if denoiser_type == 'BM3D':\n",
        "        if sigma_type == 'wavelet':\n",
        "            sigma = estimate_sigma_masked(image, mask)\n",
        "        elif sigma_type == 'empirical':\n",
        "            noise_pixels = image[mask]\n",
        "            sigma = np.sqrt(noise_pixels.var())\n",
        "        else:\n",
        "            raise ValueError(f\"Unsupported sigma_type: {sigma_type}\")\n",
        "        denoised = denoise_bm3d(image, sigma)\n",
        "        return denoised, sigma\n",
        "    elif denoiser_type == 'DRUNET':\n",
        "        if model is None or device is None:\n",
        "            raise ValueError(\"DRUNet model and device must be provided\")\n",
        "        denoised, sigma, sigma_16bit = denoise_drunet(image, mask, model, device, sigma_type=sigma_type)\n",
        "        return denoised, sigma, sigma_16bit\n",
        "    else:\n",
        "        raise ValueError(f\"Unsupported DENOISER_TYPE: {denoiser_type}\")\n",
        "\n",
        "# =============================================================================\n",
        "# Function to Process a Single Degree\n",
        "# =============================================================================\n",
        "def process_degree(degree, config):\n",
        "    print(f\"\\n===== Processing Degree {degree} =====\")\n",
        "\n",
        "    config_local = config.copy()\n",
        "    config_local['DEGREE'] = degree\n",
        "    config_local['DISTANCE'] = config_local['DISTANCE_MAP'][config_local['DATASET_TYPE']]\n",
        "    config_local['OUT_DIR_NPY'] = os.path.join(config_local['BASE_DIR'], 'Results', 'NonPC', config_local['DENOISER_TYPE'], config_local['DATASET_TYPE'], f\"deg{degree}\", f\"{config_local['DATASET_TYPE']}_Input_npy\")\n",
        "    config_local['OUT_DIR_AVG'] = os.path.join(config_local['BASE_DIR'], 'Results', 'NonPC', config_local['DENOISER_TYPE'], config_local['DATASET_TYPE'], f\"deg{degree}\", f\"{config_local['DATASET_TYPE']}_Averaged\")\n",
        "    config_local['OUT_DIR_COMBINED'] = os.path.join(config_local['BASE_DIR'], 'Results', 'NonPC', config_local['DENOISER_TYPE'], config_local['DATASET_TYPE'], f\"deg{degree}\", 'Combined')\n",
        "    config_local['OUT_DIR_DENOISED'] = os.path.join(config_local['BASE_DIR'], 'Results', 'NonPC', config_local['DENOISER_TYPE'], config_local['DATASET_TYPE'], f\"deg{degree}\", f\"Denoised_{config_local['DENOISER_TYPE']}\")\n",
        "    config_local['VIS_DIR'] = os.path.join(config_local['BASE_DIR'], 'Results', 'NonPC', config_local['DENOISER_TYPE'], config_local['DATASET_TYPE'], f\"deg{degree}\", 'Visualizations')\n",
        "    config_local['GROUND_TRUTH_DIR'] = os.path.join(config_local['BASE_DIR'], 'ground_truth', config_local['GROUND_TRUTH_MAP'][config_local['DATASET_TYPE']])\n",
        "    config_local['GROUND_TRUTH_FILE'] = f\"image1_distance{config_local['DISTANCE']}m_theta{degree}deg_FullResponse.tif\"\n",
        "    config_local['GROUND_TRUTH_PATH'] = os.path.join(config_local['GROUND_TRUTH_DIR'], config_local['GROUND_TRUTH_FILE'])\n",
        "\n",
        "    for out_dir in [config_local['OUT_DIR_NPY'], config_local['OUT_DIR_AVG'], config_local['OUT_DIR_COMBINED'], config_local['OUT_DIR_DENOISED'], config_local['VIS_DIR']]:\n",
        "        os.makedirs(out_dir, exist_ok=True)\n",
        "        print(f\"Created directory: {out_dir}\")\n",
        "\n",
        "    model = None\n",
        "    device = None\n",
        "    if config_local['DENOISER_TYPE'] == 'DRUNET':\n",
        "        current_dir = os.getcwd()\n",
        "        os.chdir(config_local['DRUNET_DIR'])\n",
        "        try:\n",
        "            from models.network_unet import UNetRes as net\n",
        "        except ImportError:\n",
        "            print(f\"Error: Cannot import UNetRes from {config_local['DRUNET_DIR']}/models/network_unet.py\")\n",
        "            return None\n",
        "        model_name = 'drunet_gray'\n",
        "        model_path = os.path.join('model_zoo', model_name + '.pth')\n",
        "        device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n",
        "        model = net(in_nc=2, out_nc=1, nc=[64, 128, 256, 512], nb=4, act_mode='R', downsample_mode=\"strideconv\", upsample_mode=\"convtranspose\")\n",
        "        try:\n",
        "            model.load_state_dict(torch.load(model_path), strict=True)\n",
        "        except Exception as e:\n",
        "            print(f\"Error loading DRUNet model from {model_path}: {e}\")\n",
        "            return None\n",
        "        model.eval()\n",
        "        for k, v in model.named_parameters():\n",
        "            v.requires_grad = False\n",
        "        model = model.to(device)\n",
        "        torch.cuda.empty_cache()\n",
        "        print(f\"Loaded DRUNet model from {model_path} on {device}\")\n",
        "        os.chdir(current_dir)\n",
        "\n",
        "    bin_nums = list(range(20))\n",
        "    if config_local['INCLUDE_FULL_RESPONSE']:\n",
        "        bin_nums.append('FullResponse')\n",
        "    num_bins = len(bin_nums)\n",
        "\n",
        "    full_stack = []\n",
        "    full_stack_file_names = []\n",
        "\n",
        "    in_dir = os.path.join(config_local['BASE_DIR'], config_local['DATASET_TYPE'])\n",
        "    total_files_converted = 0\n",
        "    for img_num in (pbar := tqdm(range(1, config_local['NUM_IMAGES'] + 1), desc=f\"Degree {degree}: Processing Images, Files Converted: 0\", leave=False)):\n",
        "        per_image_stack = []\n",
        "        per_image_file_names = []\n",
        "        for bin_num in bin_nums:\n",
        "            bin_str = f\"energyBin{bin_num}\" if isinstance(bin_num, int) else 'FullResponse'\n",
        "            in_file = f\"image{img_num}_distance{config_local['DISTANCE']}m_theta{degree}deg_{bin_str}.tif\"\n",
        "            in_path = os.path.join(in_dir, in_file)\n",
        "\n",
        "            try:\n",
        "                img = imageio.imread(in_path).astype(np.float32)\n",
        "            except FileNotFoundError:\n",
        "                print(f\"Warning: File {in_file} not found. Skipping.\")\n",
        "                continue\n",
        "\n",
        "            per_image_stack.append(img)\n",
        "            per_image_file_names.append(in_file)\n",
        "\n",
        "            out_file = in_file.replace('.tif', '.npy')\n",
        "            out_path = os.path.join(config_local['OUT_DIR_NPY'], out_file)\n",
        "            np.save(out_path, img)\n",
        "            total_files_converted += 1\n",
        "            pbar.set_description(f\"Degree {degree}: Processing Images, Files Converted: {total_files_converted}\")\n",
        "\n",
        "        final_per_image_stack = np.array(per_image_stack)\n",
        "        full_stack.append(final_per_image_stack)\n",
        "        full_stack_file_names.append(per_image_file_names)\n",
        "        print(f\"\\nStacked Image {img_num} array shape: {final_per_image_stack.shape}\")\n",
        "\n",
        "    full_array = np.array(full_stack)\n",
        "    print(f\"\\nFull-stack shape: {full_array.shape}\")\n",
        "    full_file_names_array = np.array(full_stack_file_names, dtype=object)\n",
        "    print(f\"Full-stack file names array shape: {full_file_names_array.shape}\")\n",
        "\n",
        "    if full_file_names_array.size == 0 or full_file_names_array.shape[1] == 0:\n",
        "        print(f\"No files found for degree {degree}. Skipping further processing.\")\n",
        "        return None\n",
        "\n",
        "    df = pd.DataFrame(\n",
        "        full_file_names_array,\n",
        "        index=[f'Image {i}' for i in range(1, full_file_names_array.shape[0] + 1)],\n",
        "        columns=[f'Bin {j}' if isinstance(j, int) else j for j in bin_nums]\n",
        "    )\n",
        "    print(\"\\nFile Names Matrix:\")\n",
        "    print(df.to_string())\n",
        "\n",
        "    full_stack_save_name = f\"full_stack_{config_local['NUM_IMAGES']}x{num_bins}x400x400.npy\"\n",
        "    full_stack_path = os.path.join(config_local['OUT_DIR_NPY'], full_stack_save_name)\n",
        "    np.save(full_stack_path, full_array)\n",
        "    print(f\"Saved full 4D stack: {full_stack_path}\")\n",
        "\n",
        "    full_stack_load = np.load(full_stack_path)\n",
        "    print(f\"\\nLoaded full stack shape: {full_stack_load.shape}\")\n",
        "    mean_stack = np.mean(full_stack_load, axis=0)\n",
        "    print(f\"Averaged stack shape: {mean_stack.shape}\")\n",
        "\n",
        "    for bin_num in range(mean_stack.shape[0]):\n",
        "        bin_str = f\"energyBin{bin_num}\" if isinstance(bin_num, int) else 'FullResponse'\n",
        "        img = mean_stack[bin_num].astype(np.float32)\n",
        "        out_npy = f\"averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg_{bin_str}.npy\"\n",
        "        out_npy_path = os.path.join(config_local['OUT_DIR_AVG'], out_npy)\n",
        "        np.save(out_npy_path, img)\n",
        "\n",
        "        min_val, max_val = np.min(img), np.max(img)\n",
        "        norm = (img - min_val) / (max_val - min_val + 1e-10)\n",
        "        rescaled = (norm * 65535).astype(np.uint16)\n",
        "        disp_file = f\"averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg_{bin_str}.tif\"\n",
        "        disp_path = os.path.join(config_local['OUT_DIR_AVG'], disp_file)\n",
        "        imageio.imwrite(disp_path, rescaled)\n",
        "\n",
        "    print(f\"Saved {num_bins} averaged images (.npy and .tif) for all bins in {config_local['OUT_DIR_AVG']}\")\n",
        "    avg_stack_save_name = f\"Averaged_{num_bins}x400x400.npy\"\n",
        "    avg_stack_path = os.path.join(config_local['OUT_DIR_AVG'], avg_stack_save_name)\n",
        "    np.save(avg_stack_path, mean_stack)\n",
        "    print(f\"Saved averaged stack: {avg_stack_path}\")\n",
        "\n",
        "    avg_stack_load = np.load(avg_stack_path)\n",
        "    print(f\"\\n-------------------------------------------------\")\n",
        "    print(f\"Loaded averaged stack shape: {avg_stack_load.shape}\")\n",
        "    combined_image = np.mean(avg_stack_load, axis=0)\n",
        "    print(f\"Combined Image shape: {combined_image.shape}\")\n",
        "\n",
        "    # suffix = '_withFullResponse' if config_local['INCLUDE_FULL_RESPONSE'] else '_noFullResponse'\n",
        "    suffix = ('_withFullResponse' if config_local['INCLUDE_FULL_RESPONSE'] else '_noFullResponse') + ('_anscombe' if config_local['USE_ANSCOMBE'] else '')\n",
        "    combined_npy_file = f\"combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}.npy\"\n",
        "    combined_npy_path = os.path.join(config_local['OUT_DIR_COMBINED'], combined_npy_file)\n",
        "    np.save(combined_npy_path, combined_image)\n",
        "    print(f\"-------------------------------------------------\")\n",
        "    print(f\"Saved combined image (.npy) as {combined_npy_file}\")\n",
        "\n",
        "    print(f\"min: {combined_image.min()}\")\n",
        "    print(f\"max: {combined_image.max()}\")\n",
        "\n",
        "    min_val, max_val = combined_image.min(), combined_image.max()\n",
        "    if max_val > min_val:\n",
        "        norm = (combined_image - min_val) / (max_val - min_val)\n",
        "    else:\n",
        "        norm = combined_image - min_val\n",
        "    rescaled = (norm * 65535).astype(np.uint16)\n",
        "    disp_combined_file = f\"disp_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}.tif\"\n",
        "    disp_combined_path = os.path.join(config_local['OUT_DIR_COMBINED'], disp_combined_file)\n",
        "    imageio.imwrite(disp_combined_path, rescaled)\n",
        "    print(f\"-------------------------------------------------\")\n",
        "    print(f\"Saved combined image (.tif) as {disp_combined_file}\")\n",
        "\n",
        "    print(f\"\\n-------------------------------------------------\")\n",
        "    print(\"Detecting Inner Box in Ground Truth Image\")\n",
        "    gt_img_path = config_local['GROUND_TRUTH_PATH']\n",
        "    try:\n",
        "        gt_img = imageio.imread(gt_img_path).astype(np.float32)\n",
        "        print(f\"Loaded ground truth image {gt_img_path} shape: {gt_img.shape}, dtype: {gt_img.dtype}, min: {gt_img.min()}, max: {gt_img.max()}\")\n",
        "    except FileNotFoundError:\n",
        "        print(f\"Warning: Ground truth file {gt_img_path} not found. Skipping inner box detection.\")\n",
        "        return None\n",
        "\n",
        "    inner_corners, (x_min, y_min, x_max, y_max), row_mean, col_mean = detect_inner_box(gt_img)\n",
        "    print(f\"Detected inner box coordinates: x_min={x_min}, y_min={y_min}, x_max={x_max}, y_max={y_max}\")\n",
        "    content_region = (x_min, y_min, x_max, y_max)\n",
        "    print(f\"Content region (inside white margin): {content_region}\")\n",
        "    roi_y1, roi_y2, roi_x1, roi_x2 = y_min, y_max, x_min, x_max\n",
        "    print(f\"ROI for MS-SSIM: y1={roi_y1}, y2={roi_y2}, x1={roi_x1}, x2={roi_x2}\")\n",
        "\n",
        "    fig = plt.figure(figsize=(12, 8))\n",
        "    gs = fig.add_gridspec(2, 2)\n",
        "\n",
        "    ax1 = fig.add_subplot(gs[0, 0])\n",
        "    ax1.plot(row_mean)\n",
        "    ax1.set_title(\"Row Mean Intensity\")\n",
        "    ax1.set_xlabel(\"Row Index\")\n",
        "    ax1.set_ylabel(\"Mean Intensity\")\n",
        "    ax1.axvline(x=y_min, color='r', linestyle='--', label=f'y_min={y_min}')\n",
        "    ax1.axvline(x=y_max, color='r', linestyle='--', label=f'y_max={y_max}')\n",
        "    ax1.legend()\n",
        "\n",
        "    ax2 = fig.add_subplot(gs[0, 1])\n",
        "    ax2.plot(col_mean)\n",
        "    ax2.set_title(\"Column Mean Intensity\")\n",
        "    ax2.set_xlabel(\"Column Index\")\n",
        "    ax2.set_ylabel(\"Mean Intensity\")\n",
        "    ax2.axvline(x=x_min, color='r', linestyle='--', label=f'x_min={x_min}')\n",
        "    ax2.axvline(x=x_max, color='r', linestyle='--', label=f'x_max={x_max}')\n",
        "    ax2.legend()\n",
        "\n",
        "    ax3 = fig.add_subplot(gs[1, :])\n",
        "    ax3.imshow(gt_img, cmap='gray')\n",
        "    xs = [p[0] for p in inner_corners] + [inner_corners[0][0]]\n",
        "    ys = [p[1] for p in inner_corners] + [inner_corners[0][1]]\n",
        "    ax3.plot(xs, ys, linewidth=2, color='red', label='Detected White Margin')\n",
        "    ax3.axvline(x=roi_x1, color='lime', linestyle='--', alpha=0.8, label='Content Region')\n",
        "    ax3.axvline(x=roi_x2, color='lime', linestyle='--', alpha=0.8)\n",
        "    ax3.axhline(y=roi_y1, color='lime', linestyle='--', alpha=0.8)\n",
        "    ax3.axhline(y=roi_y2, color='lime', linestyle='--', alpha=0.8)\n",
        "\n",
        "    corner_coords = [(x_min, y_min), (x_max, y_min), (x_max, y_max), (x_min, y_max)]\n",
        "    corner_labels = [f'({x_min}, {y_min})', f'({x_max}, {y_min})', f'({x_max}, {y_max})', f'({x_min}, {y_max})']\n",
        "    for (x, y), label in zip(corner_coords, corner_labels):\n",
        "        if x == x_max and y == y_max:\n",
        "            ax3.text(x - 20, y - 20, label, color='blue', fontsize=12, ha='right', va='top')\n",
        "        else:\n",
        "            ax3.text(x, y, label, color='blue', fontsize=12, ha='right', va='bottom')\n",
        "\n",
        "    ax3.legend()\n",
        "    ax3.set_title(f\"Ground Truth with Detected Regions\\nWhite Margin: ({x_min}, {y_min}) to ({x_max}, {y_max})\")\n",
        "    ax3.axis('off')\n",
        "\n",
        "    plt.tight_layout()\n",
        "    # plt.savefig(os.path.join(config_local['VIS_DIR'], f\"{config_local['DATASET_TYPE']}_deg{degree}_combined_plots.png\"), bbox_inches='tight')\n",
        "    plt.savefig(os.path.join(config_local['VIS_DIR'], f\"{config_local['DATASET_TYPE']}_deg{degree}_combined_plots{suffix}.png\"), bbox_inches='tight')\n",
        "    plt.close()\n",
        "\n",
        "    print(f\"Row mean (first 10): {row_mean[:10]}\")\n",
        "    print(f\"Row mean (last 10): {row_mean[-10:]}\")\n",
        "    print(f\"Column mean (first 10): {col_mean[:10]}\")\n",
        "    print(f\"Column mean (last 10): {col_mean[-10:]}\")\n",
        "\n",
        "    print(f\"\\n-------------------------------------------------\")\n",
        "    print(f\"Denoising Image with {config_local['DENOISER_TYPE']}\")\n",
        "    noisy_img = np.load(combined_npy_path)\n",
        "    print(f\"Loaded combined averaged image shape: {noisy_img.shape}, dtype: {noisy_img.dtype}, min: {noisy_img.min()}, max: {noisy_img.max()}\")\n",
        "\n",
        "    # Apply Anscombe transform if enabled\n",
        "    if config_local['USE_ANSCOMBE']:\n",
        "        print(\"Applying Anscombe transform\")\n",
        "        noisy_img = anscombe_transform(noisy_img)\n",
        "        print(f\"Anscombe transformed image min: {noisy_img.min()}, max: {noisy_img.max()}\")\n",
        "\n",
        "    mask = np.ones(noisy_img.shape, dtype=bool)\n",
        "    mask[roi_y1:roi_y2+1, roi_x1:roi_x2+1] = False\n",
        "\n",
        "    min_val, max_val = noisy_img.min(), noisy_img.max()\n",
        "    norm_noisy = (noisy_img - min_val) / (max_val - min_val + 1e-10) if max_val > min_val else noisy_img - min_val\n",
        "    print(f\"Input image min: {noisy_img.min()}, max: {noisy_img.max()}\")\n",
        "    print(f\"Normalized input min: {norm_noisy.min()}, max: {norm_noisy.max()}\")\n",
        "\n",
        "    if config_local['DENOISER_TYPE'] == 'BM3D':\n",
        "        denoised_wavelet, sigma_wavelet = denoise_image(norm_noisy, mask, config_local['DENOISER_TYPE'], sigma_type='wavelet')\n",
        "        norm_denoised_wavelet = (denoised_wavelet - denoised_wavelet.min()) / (denoised_wavelet.max() - denoised_wavelet.min() + 1e-10) if denoised_wavelet.max() > denoised_wavelet.min() else denoised_wavelet - denoised_wavelet.min()\n",
        "        denoised_empirical, sigma_empirical = denoise_image(norm_noisy, mask, config_local['DENOISER_TYPE'], sigma_type='empirical')\n",
        "        norm_denoised_empirical = (denoised_empirical - denoised_empirical.min()) / (denoised_empirical.max() - denoised_empirical.min() + 1e-10) if denoised_empirical.max() > denoised_empirical.min() else denoised_empirical - denoised_empirical.min()\n",
        "\n",
        "        # Apply inverse Anscombe transform if enabled\n",
        "        if config_local['USE_ANSCOMBE']:\n",
        "            print(\"Applying inverse Anscombe transform to denoised images\")\n",
        "            norm_denoised_wavelet = inverse_anscombe_transform(norm_denoised_wavelet * (max_val - min_val) + min_val)\n",
        "            norm_denoised_empirical = inverse_anscombe_transform(norm_denoised_empirical * (max_val - min_val) + min_val)\n",
        "            # Re-normalize after inverse transform\n",
        "            norm_denoised_wavelet = (norm_denoised_wavelet - norm_denoised_wavelet.min()) / (norm_denoised_wavelet.max() - norm_denoised_wavelet.min() + 1e-10) if norm_denoised_wavelet.max() > norm_denoised_wavelet.min() else norm_denoised_wavelet\n",
        "            norm_denoised_empirical = (norm_denoised_empirical - norm_denoised_empirical.min()) / (norm_denoised_empirical.max() - norm_denoised_empirical.min() + 1e-10) if norm_denoised_empirical.max() > norm_denoised_empirical.min() else norm_denoised_empirical\n",
        "\n",
        "        sigma_wavelet_0_1 = sigma_wavelet / (max_val - min_val + 1e-10) if max_val > min_val else sigma_wavelet\n",
        "        sigma_wavelet_65535 = sigma_wavelet_0_1 * 65535\n",
        "        sigma_empirical_0_1 = sigma_empirical / (max_val - min_val + 1e-10) if max_val > min_val else sigma_empirical\n",
        "        sigma_empirical_65535 = sigma_empirical_0_1 * 65535\n",
        "        sigma_rel_error = 100 * abs(sigma_empirical - sigma_wavelet) / max(sigma_wavelet, 1e-10)\n",
        "        print(f\"Wavelet-based sigma: {sigma_wavelet_0_1:.4f} (0-1), {sigma_wavelet_65535:.4f} (0-65535)\")\n",
        "        print(f\"Empirical sigma: {sigma_empirical_0_1:.4f} (0-1), {sigma_empirical_65535:.4f} (0-65535)\")\n",
        "        print(f\"Sigma Relative Error (%): {sigma_rel_error:.4f}\")\n",
        "        print(f\"Denoised (Wavelet) image min: {norm_denoised_wavelet.min():.4f}\")\n",
        "        print(f\"Denoised (Wavelet) image max: {norm_denoised_wavelet.max():.4f}\")\n",
        "        print(f\"Denoised (Empirical) image min: {norm_denoised_empirical.min():.4f}\")\n",
        "        print(f\"Denoised (Empirical) image max: {norm_denoised_empirical.max():.4f}\")\n",
        "        denoised_npy_file_wavelet = f\"denoised_{config_local['DENOISER_TYPE'].lower()}_wavelet_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.npy\"\n",
        "        denoised_npy_path_wavelet = os.path.join(config_local['OUT_DIR_DENOISED'], denoised_npy_file_wavelet)\n",
        "        np.save(denoised_npy_path_wavelet, norm_denoised_wavelet)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (wavelet) image (.npy) as {denoised_npy_file_wavelet}\")\n",
        "        rescaled_denoised_wavelet = (norm_denoised_wavelet * 65535).astype(np.uint16)\n",
        "        disp_denoised_file_wavelet = f\"disp_denoised_{config_local['DENOISER_TYPE'].lower()}_wavelet_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.tif\"\n",
        "        disp_denoised_path_wavelet = os.path.join(config_local['OUT_DIR_DENOISED'], disp_denoised_file_wavelet)\n",
        "        imageio.imwrite(disp_denoised_path_wavelet, rescaled_denoised_wavelet)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (wavelet) image (.tif) as {disp_denoised_file_wavelet}\")\n",
        "        denoised_npy_file_empirical = f\"denoised_{config_local['DENOISER_TYPE'].lower()}_empirical_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.npy\"\n",
        "        denoised_npy_path_empirical = os.path.join(config_local['OUT_DIR_DENOISED'], denoised_npy_file_empirical)\n",
        "        np.save(denoised_npy_path_empirical, norm_denoised_empirical)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (empirical) image (.npy) as {denoised_npy_file_empirical}\")\n",
        "        rescaled_denoised_empirical = (norm_denoised_empirical * 65535).astype(np.uint16)\n",
        "        disp_denoised_file_empirical = f\"disp_denoised_{config_local['DENOISER_TYPE'].lower()}_empirical_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.tif\"\n",
        "        disp_denoised_path_empirical = os.path.join(config_local['OUT_DIR_DENOISED'], disp_denoised_file_empirical)\n",
        "        imageio.imwrite(disp_denoised_path_empirical, rescaled_denoised_empirical)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (empirical) image (.tif) as {disp_denoised_file_empirical}\")\n",
        "    else:\n",
        "        denoised_wavelet, sigma_wavelet, sigma_wavelet_16bit = denoise_image(norm_noisy, mask, config_local['DENOISER_TYPE'], sigma_type='wavelet', model=model, device=device)\n",
        "        norm_denoised_wavelet = (denoised_wavelet - denoised_wavelet.min()) / (denoised_wavelet.max() - denoised_wavelet.min() + 1e-10) if denoised_wavelet.max() > denoised_wavelet.min() else denoised_wavelet\n",
        "        denoised_empirical, sigma_empirical, sigma_empirical_16bit = denoise_image(norm_noisy, mask, config_local['DENOISER_TYPE'], sigma_type='empirical', model=model, device=device)\n",
        "        norm_denoised_empirical = (denoised_empirical - denoised_empirical.min()) / (denoised_empirical.max() - denoised_empirical.min() + 1e-10) if denoised_empirical.max() > denoised_empirical.min() else denoised_empirical\n",
        "\n",
        "        # Apply inverse Anscombe transform if enabled\n",
        "        if config_local['USE_ANSCOMBE']:\n",
        "            print(\"Applying inverse Anscombe transform to denoised images\")\n",
        "            norm_denoised_wavelet = inverse_anscombe_transform(norm_denoised_wavelet * (max_val - min_val) + min_val)\n",
        "            norm_denoised_empirical = inverse_anscombe_transform(norm_denoised_empirical * (max_val - min_val) + min_val)\n",
        "            # Re-normalize after inverse transform\n",
        "            norm_denoised_wavelet = (norm_denoised_wavelet - norm_denoised_wavelet.min()) / (norm_denoised_wavelet.max() - norm_denoised_wavelet.min() + 1e-10) if norm_denoised_wavelet.max() > norm_denoised_wavelet.min() else norm_denoised_wavelet\n",
        "            norm_denoised_empirical = (norm_denoised_empirical - norm_denoised_empirical.min()) / (norm_denoised_empirical.max() - norm_denoised_empirical.min() + 1e-10) if norm_denoised_empirical.max() > norm_denoised_empirical.min() else norm_denoised_empirical\n",
        "\n",
        "        # Compute sigmas on original image scale for CSV reporting\n",
        "        sigma_wavelet_orig = estimate_sigma_masked(noisy_img, mask)\n",
        "        sigma_empirical_orig = np.sqrt(noisy_img[mask].var())\n",
        "        sigma_wavelet_0_1 = sigma_wavelet_orig / (max_val - min_val + 1e-10) if max_val > min_val else sigma_wavelet_orig\n",
        "        sigma_wavelet_65535 = sigma_wavelet_0_1 * 65535.0\n",
        "        sigma_empirical_0_1 = sigma_empirical_orig / (max_val - min_val + 1e-10) if max_val > min_val else sigma_empirical_orig\n",
        "        sigma_empirical_65535 = sigma_empirical_0_1 * 65535.0\n",
        "        sigma_rel_error = 100 * abs(sigma_empirical_orig - sigma_wavelet_orig) / max(sigma_wavelet_orig, 1e-10)\n",
        "        print(f\"Wavelet-based sigma: {sigma_wavelet_0_1:.4f} (0-1), {sigma_wavelet_65535:.4f} (0-65535)\")\n",
        "        print(f\"Empirical sigma: {sigma_empirical_0_1:.4f} (0-1), {sigma_empirical_65535:.4f} (0-65535)\")\n",
        "        print(f\"Sigma Relative Error (%): {sigma_rel_error:.4f}\")\n",
        "        print(f\"Denoised (Wavelet) image min: {norm_denoised_wavelet.min():.4f}\")\n",
        "        print(f\"Denoised (Wavelet) image max: {norm_denoised_wavelet.max():.4f}\")\n",
        "        print(f\"Denoised (Empirical) image min: {norm_denoised_empirical.min():.4f}\")\n",
        "        print(f\"Denoised (Empirical) image max: {norm_denoised_empirical.max():.4f}\")\n",
        "        denoised_npy_file_wavelet = f\"denoised_{config_local['DENOISER_TYPE'].lower()}_wavelet_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.npy\"\n",
        "        denoised_npy_path_wavelet = os.path.join(config_local['OUT_DIR_DENOISED'], denoised_npy_file_wavelet)\n",
        "        np.save(denoised_npy_path_wavelet, norm_denoised_wavelet)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (wavelet) image (.npy) as {denoised_npy_file_wavelet}\")\n",
        "        rescaled_denoised_wavelet = (norm_denoised_wavelet * 65535).astype(np.uint16)\n",
        "        disp_denoised_file_wavelet = f\"disp_denoised_{config_local['DENOISER_TYPE'].lower()}_wavelet_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.tif\"\n",
        "        disp_denoised_path_wavelet = os.path.join(config_local['OUT_DIR_DENOISED'], disp_denoised_file_wavelet)\n",
        "        imageio.imwrite(disp_denoised_path_wavelet, rescaled_denoised_wavelet)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (wavelet) image (.tif) as {disp_denoised_file_wavelet}\")\n",
        "        denoised_npy_file_empirical = f\"denoised_{config_local['DENOISER_TYPE'].lower()}_empirical_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.npy\"\n",
        "        denoised_npy_path_empirical = os.path.join(config_local['OUT_DIR_DENOISED'], denoised_npy_file_empirical)\n",
        "        np.save(denoised_npy_path_empirical, norm_denoised_empirical)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (empirical) image (.npy) as {denoised_npy_file_empirical}\")\n",
        "        rescaled_denoised_empirical = (norm_denoised_empirical * 65535).astype(np.uint16)\n",
        "        disp_denoised_file_empirical = f\"disp_denoised_{config_local['DENOISER_TYPE'].lower()}_empirical_combined_averaged_image_distance{config_local['DISTANCE']}m_theta{degree}deg{suffix}{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.tif\"\n",
        "        disp_denoised_path_empirical = os.path.join(config_local['OUT_DIR_DENOISED'], disp_denoised_file_empirical)\n",
        "        imageio.imwrite(disp_denoised_path_empirical, rescaled_denoised_empirical)\n",
        "        print(f\"-------------------------------------------------\")\n",
        "        print(f\"Saved denoised (empirical) image (.tif) as {disp_denoised_file_empirical}\")\n",
        "\n",
        "    min_gt, max_gt = gt_img.min(), gt_img.max()\n",
        "    norm_gt = (gt_img - min_gt) / (max_gt - min_gt + 1e-10)\n",
        "    print(f\"Ground truth min: {gt_img.min()}, max: {gt_img.max()}\")\n",
        "    gt_crop = norm_gt[roi_y1:roi_y2+1, roi_x1:roi_x2+1]\n",
        "    noisy_crop = norm_noisy[roi_y1:roi_y2+1, roi_x1:roi_x2+1]\n",
        "    print(f\"Cropped image shape: {gt_crop.shape}\")\n",
        "\n",
        "    den_crop_wavelet = norm_denoised_wavelet[roi_y1:roi_y2+1, roi_x1:roi_x2+1]\n",
        "    den_crop_empirical = norm_denoised_empirical[roi_y1:roi_y2+1, roi_x1:roi_x2+1]\n",
        "    gt_tensor = torch.from_numpy(norm_gt).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    den_tensor_wavelet = torch.from_numpy(norm_denoised_wavelet).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    den_tensor_empirical = torch.from_numpy(norm_denoised_empirical).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    noisy_tensor = torch.from_numpy(norm_noisy).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    msssim_noisy = ms_ssim(noisy_tensor, gt_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    msssim_denoised_wavelet = ms_ssim(den_tensor_wavelet, gt_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    msssim_denoised_empirical = ms_ssim(den_tensor_empirical, gt_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    print(f\"-------------------------------------------------\")\n",
        "    print(f\"MS-SSIM (Full Image, Noisy): {msssim_noisy:.4f}\")\n",
        "    print(f\"MS-SSIM (Full Image, Denoised Wavelet): {msssim_denoised_wavelet:.4f}\")\n",
        "    print(f\"MS-SSIM (Full Image, Denoised Empirical): {msssim_denoised_empirical:.4f}\")\n",
        "    gt_crop_tensor = torch.from_numpy(gt_crop).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    den_crop_tensor_wavelet = torch.from_numpy(den_crop_wavelet).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    den_crop_tensor_empirical = torch.from_numpy(den_crop_empirical).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    noisy_crop_tensor = torch.from_numpy(noisy_crop).unsqueeze(0).unsqueeze(0).float().to(device if config_local['DENOISER_TYPE'] == 'DRUNET' else 'cpu')\n",
        "    msssim_noisy_crop = ms_ssim(noisy_crop_tensor, gt_crop_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    msssim_denoised_crop_wavelet = ms_ssim(den_crop_tensor_wavelet, gt_crop_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    msssim_denoised_crop_empirical = ms_ssim(den_crop_tensor_empirical, gt_crop_tensor, data_range=config_local['MS_SSIM_DATA_RANGE'], size_average=True).item()\n",
        "    print(f\"MS-SSIM (Cropped Image, Noisy): {msssim_noisy_crop:.4f}\")\n",
        "    print(f\"MS-SSIM (Cropped Image, Denoised Wavelet): {msssim_denoised_crop_wavelet:.4f}\")\n",
        "    print(f\"MS-SSIM (Cropped Image, Denoised Empirical): {msssim_denoised_crop_empirical:.4f}\")\n",
        "    psnr_noisy = psnr(norm_gt, norm_noisy, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    psnr_denoised_wavelet = psnr(norm_gt, norm_denoised_wavelet, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    psnr_denoised_empirical = psnr(norm_gt, norm_denoised_empirical, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    psnr_noisy_crop = psnr(gt_crop, noisy_crop, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    psnr_denoised_crop_wavelet = psnr(gt_crop, den_crop_wavelet, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    psnr_denoised_crop_empirical = psnr(gt_crop, den_crop_empirical, max_pixel=config_local['MS_SSIM_DATA_RANGE'])\n",
        "    print(f\"PSNR (Full Image, Noisy): {psnr_noisy:.4f}\")\n",
        "    print(f\"PSNR (Full Image, Denoised Wavelet): {psnr_denoised_wavelet:.4f}\")\n",
        "    print(f\"PSNR (Full Image, Denoised Empirical): {psnr_denoised_empirical:.4f}\")\n",
        "    print(f\"PSNR (Cropped Image, Noisy): {psnr_noisy_crop:.4f}\")\n",
        "    print(f\"PSNR (Cropped Image, Denoised Wavelet): {psnr_denoised_crop_wavelet:.4f}\")\n",
        "    print(f\"PSNR (Cropped Image, Denoised Empirical): {psnr_denoised_crop_empirical:.4f}\")\n",
        "\n",
        "    if msssim_denoised_crop_wavelet >= msssim_denoised_crop_empirical:\n",
        "        best_denoised = norm_denoised_wavelet\n",
        "        best_denoised_type = 'Wavelet'\n",
        "    else:\n",
        "        best_denoised = norm_denoised_empirical\n",
        "        best_denoised_type = 'Empirical'\n",
        "\n",
        "    fig, axes = plt.subplots(1, 3, figsize=(15, 6))\n",
        "    titles = ['Ground Truth', 'Noisy (Normalized)', f'Denoised ({best_denoised_type})']\n",
        "    for ax, data, title in zip(axes, [gt_img, norm_noisy, best_denoised], titles):\n",
        "        ax.imshow(data, cmap='gray')\n",
        "        ax.set_title(title, fontsize=12, pad=15)\n",
        "        ax.axis('off')\n",
        "    plt.tight_layout(pad=2.0)\n",
        "    # plt.savefig(os.path.join(config_local['VIS_DIR'], f\"{config_local['DATASET_TYPE']}_deg{degree}_comparison{'_anscombe' if config_local['USE_ANSCOMBE'] else ''}.png\"), bbox_inches='tight')\n",
        "    plt.savefig(os.path.join(config_local['VIS_DIR'], f\"{config_local['DATASET_TYPE']}_deg{degree}_comparison{suffix}.png\"), bbox_inches='tight')\n",
        "    plt.close()\n",
        "\n",
        "    return {\n",
        "        'Dataset Type': config_local['DATASET_TYPE'],\n",
